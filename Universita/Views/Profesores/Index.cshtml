@model IEnumerable<Servuniv.Data.Entities.Profesores>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Identificacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombres)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellidos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nacimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ingreso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Trabaja)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Condiciones)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProfesoresTipos)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Identificacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombres)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellidos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nacimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ingreso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Trabaja)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Condiciones.Condicion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProfesoresTipos.Tipodeprofesor)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idprofesor">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idprofesor">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idprofesor">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
